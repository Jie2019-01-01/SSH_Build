<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.erp.auth.emp.vo.EmpModel" table="tbl_emp">
		<id name="uuid">
			<generator class="native"/>
		</id>
		<property name="userName"/>
		<property name="pwd"/>
		<property name="name"/>
		<property name="email"/>
		<property name="tele"/>
		<property name="address"/>
		<property name="gender"/>
		<property name="birthDay"/>
		<property name="lastLoginIp"/>
        <property name="lastLoginTime"/>
        <property name="loginTimes"/>
		<many-to-one name="dm" column="depUuid" class="cn.itcast.erp.auth.dep.vo.DepModel" lazy="false" fetch="join"/>
	</class>
</hibernate-mapping>